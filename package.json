{
  "name": "eosjs",
  "version": "21.0.1-rc2",
  "description": "Talk to eos API",
  "main": "dist/index.js",
  "scripts": {
    "cypress": "cypress run --spec 'cypress/integration/index.spec.js'",
    "cypress-ui": "cypress open",
    "prepare": "npm run build",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "test": "jest src/tests/*eosjs*",
    "test-node": "jest src/tests/*node*",
    "test-all": "yarn test && yarn test-node && yarn cypress",
    "build": "tsc -p ./tsconfig.json && cp src/ripemd.es5.js dist/ripemd.js",
    "build-web": "webpack --config webpack.prod.js && webpack --config webpack.debug.js",
    "build-production": "yarn build && yarn build-web && yarn test-all",
    "clean": "rm -rf dist",
    "docs": "yarn typedoc"
  },
  "author": "block.one",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EOSIO/eosjs.git"
  },
  "dependencies": {
    "elliptic": "6.5.2",
    "pako": "1.0.11"
  },
  "devDependencies": {
    "@blockone/tslint-config-blockone": "^3.1.0",
    "@types/elliptic": "6.4.12",
    "@types/jest": "^24.9.1",
    "@types/node": "^11.15.16",
    "@types/pako": "^1.0.1",
    "cypress": "^3.8.3",
    "eosjs-ecc": "^4.0.7",
    "jest": "^25.5.4",
    "jest-fetch-mock": "^2.1.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^5.4.5",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "resolutions": {
    "minimist": "1.2.5"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./src/tests/setupJest.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(tsx?)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testRegex": "(/src/.*(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node"
  }
}
